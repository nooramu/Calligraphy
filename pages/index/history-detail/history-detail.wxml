<view class="container">
  <view class="detail-card">
    <image src="{{detail.imagePath}}" mode="aspectFit" class="detail-image"/>
    <view class="detail-info">
      <view class="style-name">{{detail.styleName}}</view>
      <view class="analyze-time">{{detail.analyzeTime}}</view>
      <view class="confidence">置信度：{{detail.confidence}}%</view>
      <view class="description">{{detail.description}}</view>
    </view>
  </view>
</view> 